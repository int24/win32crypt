{
    "name": "win32crypt",
    "version": "0.2.0",
    "description": "Windows DPAPI in Node.js.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "author": "Braden Lamb <braden@int24.io>",
    "license": "UNLICENSED",
    "private": false,
    "scripts": {
