        "eslint": "8.20.0",
        "husky": "8.0.1",
        "lint-staged": "13.0.3",
        "prettier": "2.7.1",
        "typescript": "4.7.4"
    },
    "lint-staged": {
        "*.{js,ts,yml,yaml,json}": "prettier --write",
        "*.{js,ts}": "eslint --fix"
    }
